{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "__DEVELOPMENT__": true,
    "__PRODUCTION__": true,
    "expect": true
  },
  "rules": {
    "max-len": 0,
    "no-console": 0,
    "no-unused-expressions": 0,
    "no-param-reassign": 0,
    "no-unneeded-ternary": 0,
    "arrow-body-style": 0,
    "default-case": 0,
    "prefer-template": 0,
    "padded-blocks": 0,
    "camelcase": 0,
    "no-else-return": 0,
    "func-names": 0,
    "consistent-return": 0,
    "array-callback-return": 0,
    "no-confusing-arrow": 0,
    "no-empty": 0,
    "no-nested-ternary": 0,
    "no-loop-func": 0,
    "no-whitespace-before-property": 0,
    "prefer-arrow-callback": 0,
    "no-case-declarations": 0,
    "no-alert": 0,
    "import/newline-after-import": 0,
    "import/no-extraneous-dependencies": 0,
    "import/first": 0,
    "import/extensions": 0,
    "global-require": 0,
    "arrow-parens": 0,
    "jsx-a11y/href-no-hash": 0,
    "jsx-a11y/img-has-alt": 0,
    "no-plusplus": 0,
    "no-bitwise": 0,
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0,
    "no-path-concat": 0
  }
}
